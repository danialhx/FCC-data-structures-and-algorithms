VV  //this function is a blueprint 
function Person(first, last, age, eye) {
  this.firstName = first;
  this.lastName = last;
  this.age = age;
  this.eyeColor = eye;
}

// a blue print of an object is known as a constructor

var myFather = new Person("John", "Doe", 50, "blue");


